<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.com.cnpc.cpoa.mapper.contractor.BizContCreditSetItemMapper">
    <resultMap id="BaseResultMap" type="cn.com.cnpc.cpoa.domain.contractor.BizContCreditSetItemDto">
        <!--
          WARNING - @mbg.generated
        -->
        <result column="ITEM_ID" jdbcType="VARCHAR" property="itemId"/>
        <result column="SET_ID" jdbcType="VARCHAR" property="setId"/>
        <result column="CREDIT_ID" jdbcType="VARCHAR" property="creditId"/>
    </resultMap>

    <!--查询-->
    <select id="selectContCreditSetItem" parameterType="java.util.Map" resultMap="BaseResultMap">
        SELECT * FROM T_CONT_CREDIT_SET_ITEM TCCSI

        <where>
            <if test="creditId!=null and creditId!=''">
                and TCCSI.CREDIT_ID=#{creditId}
            </if>

        </where>

    </select>

</mapper>