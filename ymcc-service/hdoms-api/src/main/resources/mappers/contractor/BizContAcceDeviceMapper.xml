<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.com.cnpc.cpoa.mapper.contractor.BizContAcceDeviceMapper">

  <resultMap id="BaseResultMap" type="cn.com.cnpc.cpoa.domain.contractor.BizContAcceDeviceDto">
    <result column="DEV_ID" jdbcType="VARCHAR" property="devId" />
    <result column="ACCE_ID" jdbcType="VARCHAR" property="acceId" />
    <result column="DEV_NO" jdbcType="INTEGER" property="devNo" />
    <result column="DEV_NAME" jdbcType="VARCHAR" property="devName" />
    <result column="DEV_TYPE" jdbcType="VARCHAR" property="devType" />
    <result column="DEV_TOTAL" jdbcType="INTEGER" property="devTotal" />
    <result column="DEV_STATUS" jdbcType="VARCHAR" property="devStatus" />
  </resultMap>

  <select id="selectContAcceDeviceDto" parameterType="java.util.Map" resultMap="BaseResultMap">
    SELECT * FROM T_CONT_ACCE_DEVICE CAD
    <where>
      <if test="acceId!=null and acceId!=''">
        and CAD.ACCE_ID=#{acceId}
      </if>
    </where>

  </select>





</mapper>