<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.com.cnpc.cpoa.mapper.prodsys.SysSubDeptDtoMapper">
    <resultMap id="BaseResultMap" type="cn.com.cnpc.cpoa.domain.prodsys.SysSubDeptDto">
        <!--
          WARNING - @mbg.generated
        -->
        <result column="SUB_DEPT_ID" jdbcType="VARCHAR" property="subDeptId"/>
        <result column="SUB_DEPT_NAME" jdbcType="VARCHAR" property="subDeptName"/>
        <result column="DEPT_ID" jdbcType="VARCHAR" property="deptId"/>
        <result column="DEPT_NAME" jdbcType="VARCHAR" property="deptName"/>
        <result column="REF_DEPT_ID" jdbcType="VARCHAR" property="refDeptId"/>
    </resultMap>

    <select id="selectList" parameterType="java.util.Map" resultMap="BaseResultMap">
        SELECT TDS.*
        FROM T_SYS_DEPT_SUB TDS
        <where>
            <if test="refDeptId!=null and refDeptId!=''">
                AND TDS.REF_DEPT_ID = #{refDeptId}
            </if>
            <if test="subDeptId!=null and subDeptId!=''">
                AND TDS.SUB_DEPT_ID = #{subDeptId}
            </if>
        </where>
    </select>
</mapper>
