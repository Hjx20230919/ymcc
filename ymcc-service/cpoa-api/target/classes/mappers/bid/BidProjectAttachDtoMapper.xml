<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.com.cnpc.cpoa.mapper.bid.BidProjectAttachDtoMapper">
  <resultMap id="BaseResultMap" type="cn.com.cnpc.cpoa.domain.bid.BidProjectAttachDto">
    <!--
      WARNING - @mbg.generated
    -->
    <result column="ID" jdbcType="VARCHAR" property="id" />
    <result column="BID_PROJ_ID" jdbcType="VARCHAR" property="bidProjId" />
    <result column="ATTACH_ID" jdbcType="VARCHAR" property="attachId" />
  </resultMap>

  <!--查询附件-->
  <select id="selectAttachDto" parameterType="java.util.Map" resultMap="BaseResultMap">
    SELECT * FROM T_BID_PROJECT_ATTACH bpa
    <where>
      <if test="bidProjId !=null and bidProjId !=''">
        and bpa.BID_PROJ_ID=#{bidProjId}
      </if>

    </where>
  </select>

  <!--查询附件-->
  <select id="selectByAttachId" parameterType="java.lang.String" resultType="java.lang.String">
    SELECT ID FROM T_BID_PROJECT_ATTACH bpa
    <where>
      <if test="attachId !=null and attachId !=''">
        and bpa.ATTACH_ID=#{attachId}
      </if>

    </where>
  </select>
</mapper>