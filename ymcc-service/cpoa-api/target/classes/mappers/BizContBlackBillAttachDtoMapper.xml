<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.com.cnpc.cpoa.mapper.BizContBlackBillAttachDtoMapper">
  <resultMap id="BaseResultMap" type="cn.com.cnpc.cpoa.domain.BizContBlackBillAttachDto">
    <result column="BLACKBILL_ATTACH_ID" jdbcType="VARCHAR" property="blackbillAttachId" />
    <result column="BLACKLIST_ID" jdbcType="VARCHAR" property="blacklistId" />
    <result column="ATTACH_ID" jdbcType="VARCHAR" property="attachId" />
  </resultMap>

  <select id="selectAllByBlackListId" parameterType="java.lang.String" resultMap="BaseResultMap">
    select * from T_CONT_BLACKBILL_ATTACH
    <where>
      <if test="blackListId != null and blackListId != ''">
        and BLACKLIST_ID = #{blackListId}
      </if>
    </where>
  </select>
</mapper>