<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.com.cnpc.cpoa.mapper.BizCategoryDtoMapper">
  <resultMap id="BaseResultMap" type="cn.com.cnpc.cpoa.domain.BizCategoryDto">
    <!--
      WARNING - @mbg.generated
    -->
    <result column="CATEGORY_ID" jdbcType="VARCHAR" property="categoryId" />
    <result column="CATEGORY_NAME" jdbcType="VARCHAR" property="categoryName" />
    <result column="NOTES" jdbcType="VARCHAR" property="notes" />

  </resultMap>
  <!--查询合同类别-->
  <select id="selectList" parameterType="java.util.Map" resultMap="BaseResultMap">
  select * from  T_BIZ_CATEGORY TBC
  <where>
    <if test="categoryName!=null and categoryName!=''">
      AND TBC.CATEGORY_NAME like    CONCAT('%',#{categoryName},'%')
    </if>

  </where>

  </select>
  <!--查询合同类别-->
  <select id="selectListByName" parameterType="java.util.Map" resultMap="BaseResultMap">
  select * from  T_BIZ_CATEGORY TBC WHERE TBC.CATEGORY_NAME = #{categoryName}
  </select>
</mapper>